<launch>
    <rosparam command="load" file="$(find scene_setup)/config/scene_params.yaml"/>
    <rosparam command="load" file="$(find greedy_search)/config/block_locations.yaml"/>

    <node name="fake_search" pkg="greedy_search" type="fake_search" output="screen"/>
    
    <include file="$(find manipulator_control)/launch/manipulator.launch">
        <arg name="sim" value="true"/>
    </include>

    <node name="scene_setup" pkg="scene_setup" type="scene_setup" output="screen"/>
</launch>